{
  "name": "venom9078/nova-dependable-panel",
  "description": "A Laravel Nova Field acting as a Panel for grouped dependsOn functionality",
  "keywords": [
    "laravel",
    "nova",
    "panel",
    "dependsOn"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.3|^8.0",
    "laravel/nova": "^4.0 || ^5.0",
    "nova-kit/nova-packages-tool": "^1.0",
    "venom9078/nova-subfield-dependson": "@dev"
  },
  "autoload": {
    "psr-4": {
      "Formfeed\\DependablePanel\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Formfeed\\DependablePanel\\FieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    },
    {
      "type": "vcs",
      "url": "https://github.com/VenoM9078/nova-subfield-dependson"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true
}
